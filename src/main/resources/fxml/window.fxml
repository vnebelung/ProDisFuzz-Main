<?import javafx.scene.control.ScrollPane?><?import javafx.scene.layout.VBox?><?import view.controls.ControlBar?><?import view.log.Log?><?import java.net.URL?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="javafx.scene.layout.VBox" id="window">
    <children>
        <VBox fx:id="mainFrame" VBox.vgrow="ALWAYS">
            <ScrollPane fx:id="scrollPane" content="$null" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER"
                        VBox.vgrow="ALWAYS" />
            <ControlBar fx:id="controlBar" />
        </VBox>
        <Log />
    </children>
    <stylesheets>
        <URL value="@../css/window.css" />
    </stylesheets>
</fx:root>
